{
  "version": 2,
  "builds": [
    { "src": "api/receber-form.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
 "routes": [
  { "src": "/api/(.*)", "dest": "/api/$1" },
  { "src": "/(.*\\.css)$", "dest": "/$1" },
  { "src": "/(.*\\.js)$", "dest": "/$1" },
  { "src": "/(.*)", "dest": "/index.html" }
]

}
